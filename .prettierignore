# Ignore markdown files
*.md

# Ignore documentation files
docs/

# Ignore build outputs
dist/
build/
.astro/

# Ignore dependencies
node_modules/

# Ignore environment files
.env
.env.local
.env.*.local

# Ignore lock files
package-lock.json
yarn.lock
pnpm-lock.yaml

# Ignore generated files
*.min.js
*.min.css

# Ignore test coverage
coverage/

# Ignore logs
*.log

# Ignore cache
.cache/
.temp/
